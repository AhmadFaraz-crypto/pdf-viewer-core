{
  "name": "pdf-viewer-core",
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AhmadFaraz-crypto/pdf-viewer-core.git"
  },
  "bugs": {
    "url": "https://github.com/AhmadFaraz-crypto/pdf-viewer-core/issues"
  },
  "homepage": "https://github.com/AhmadFaraz-crypto/pdf-viewer-core#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "vite build --watch",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:lint": "eslint .",
    "test:types": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "example": "cd example && npm install && npm run dev",
    "example:build": "cd example && npm install && npm run build",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "react",
    "pdf",
    "viewer",
    "component",
    "typescript"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.0",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jsdom": "^22.1.0",
    "path": "^0.12.7",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^0.34.0"
  },
  "author": "Ahmad Faraz",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
